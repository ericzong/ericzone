    "version": "0.11.17",
    "architecture": {
        "64bit": {
            "url": "https://typora.io/windows/typora-setup-x64.exe",
            "hash": "f1eedc9cc89f8751d4bb77f603f5dabc26364d58116c211fe65ce002dc40be60"
        },
        "32bit": {
            "url": "https://typora.io/windows/typora-setup-ia32.exe",
            "hash": "1a529845996534b87d43a9c3727d151ccfdd2558fe835bd79569e61408bec3b9"
        }
    },
    "installer": {
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/NOCANCEL",
            "/NORESTART",
            "/CLOSEAPPLICATIONS",
            "/GROUP=typora",
            "/DIR=$dir",
            "/LOG=$dir/install.log"
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": "/VERYSILENT"
    },
    "bin": "Typora.exe",
    "checkver": {
        "url": "https://typora.io/windows/dev_release.html",
        "regex": "<h4>([\\d\\.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://typora.io/windows/typora-setup-x64.exe"
            },
            "32bit": {
                "url": "https://typora.io/windows/typora-setup-ia32.exe"
            }
        }
    },
    "shortcuts": [
        [
            "Typora.exe",
            "Typora"
        ]
    ]
}
