    "version": "0.10.10",
    "architecture": {
        "64bit": {
            "url": "https://typora.io/windows/typora-setup-x64.exe",
            "hash": "182ec2d9a40225478c1e5472aa2ddec061aeb6df7d7fcd50f12112e7f39413e3"
        },
        "32bit": {
            "url": "https://typora.io/windows/typora-setup-ia32.exe",
            "hash": "3f0510b22cc7c571273425b18319cb53a9d6e03e67fd2668b57803976a561009"
        }
    },
    "installer": {
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/NOCANCEL",
            "/NORESTART",
            "/CLOSEAPPLICATIONS",
            "/GROUP=typora",
            "/DIR=$dir",
            "/LOG=$dir/install.log"
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": "/VERYSILENT"
    },
    "bin": "Typora.exe",
    "checkver": {
        "url": "https://typora.io/windows/dev_release.html",
        "regex": "<h4>([\\d\\.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://typora.io/windows/typora-setup-x64.exe"
            },
            "32bit": {
                "url": "https://typora.io/windows/typora-setup-ia32.exe"
            }
        }
    },
    "shortcuts": [
        [
            "Typora.exe",
            "Typora"
        ]
    ]
}
