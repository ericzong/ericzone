        },
        "32bit": {
            "url": "https://typora.io/windows/typora-setup-ia32.exe",
            "hash": "20fab75b2575fbbeffad664b571df6c9a0d97266ca36fe32ff0e9d160daa7136"
        }
    },
    "installer": {
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/NOCANCEL",
            "/NORESTART",
            "/CLOSEAPPLICATIONS",
            "/GROUP=typora",
            "/DIR=$dir",
            "/LOG=$dir/install.log"
        ]
    },
    "uninstaller": {
        "file": "unins000.exe",
        "args": "/VERYSILENT"
    },
    "bin": "Typora.exe",
    "checkver": {
        "url": "https://typora.io/windows/dev_release.html",
        "regex": "<h4>([\\d\\.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://typora.io/windows/typora-setup-x64.exe"
            },
            "32bit": {
                "url": "https://typora.io/windows/typora-setup-ia32.exe"
            }
        }
    },
    "shortcuts": [
        [
            "Typora.exe",
            "Typora"
        ]
    ]
}
