    "installer": {
        "args": [
            "/VERYSILENT",
            "/SUPPRESSMSGBOXES",
            "/NOCANCEL",
            "/NORESTART",
            "/CLOSEAPPLICATIONS",
            "/GROUP=typora",
            "/DIR=$dir",
            "/LOG=$dir/install.log"
        ]
