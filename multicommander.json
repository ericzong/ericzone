{
    "homepage": "http://multicommander.com/",
    "version": "9.2.0",
    "description": "Multi Commander is a multi-tabbed file manager and is an alternative to the standard Windows Explorer. ",
    "license": {
        "identifier": "Freeware"
    },
    "persist": [
        "Config",
        "UserData",
        "SessionConfig"
    ],
    "architecture": {
        "64bit": {
            "url": "http://multicommander.com/files/updates/MultiCommander_x64_Portable_(9.2.0.2555).zip",
            "hash": "sha1:01afe61ceab75a3b8e9b2931bf6ee8f73bc065cc"
        },
        "32bit": {
            "url": "http://multicommander.com/files/updates/MultiCommander_win32_Portable_(9.2.0.2555).zip",
            "hash": "sha1:63fc6608c71e2a780033da5df7096212a4741d92"
        }
    },
    "checkver": {
        "url": "http://multicommander.com/downloads",
        "re": "MultiCommander v(?<version>[\\d.]+) \\(Build (?<build>[\\d]{4})\\)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://multicommander.com/files/updates/MultiCommander_x64_Portable_($version.$matchBuild).zip",
                "hash": {
                    "url": "http://multicommander.com/downloads",
                    "find": "(?:Portable).*\\(64 bit\\).*?([a-fA-F0-9]{40})"
                }
            },
            "32bit": {
                "url": "http://multicommander.com/files/updates/MultiCommander_win32_Portable_($version.$matchBuild).zip",
                "hash": {
                    "url": "http://multicommander.com/downloads",
                    "find": "(?:Portable).*\\(32 bit\\).*?([a-fA-F0-9]{40})"
                }
            }
        }
    }
}
