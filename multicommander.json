{
    "homepage": "http://multicommander.com/",
    "version": "9.0.0",
    "description": "Multi Commander is a multi-tabbed file manager and is an alternative to the standard Windows Explorer. ",
    "license": {
        "identifier": "Freeware"
    },
    "persist": [
        "Config",
        "UserData",
        "SessionConfig"
    ],
    "architecture": {
        "64bit": {
            "url": "http://multicommander.com/files/updates/MultiCommander_x64_Portable_(9.0.0.2532).zip",
            "hash": "sha1:3b4fb14e046fa61ecc9b5c9a5788ff437ccaebd3"
        },
        "32bit": {
            "url": "http://multicommander.com/files/updates/MultiCommander_win32_Portable_(9.0.0.2532).zip",
            "hash": "sha1:c49fddfe4e1cdd37510ed0ae83fc6c8798116798"
        }
    },
    "checkver": {
        "url": "http://multicommander.com/downloads",
        "re": "MultiCommander v(?<version>[\\d.]+) \\(Build (?<build>[\\d]{4})\\)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://multicommander.com/files/updates/MultiCommander_x64_Portable_($version.$matchBuild).zip",
                "hash": {
                    "url": "http://multicommander.com/downloads",
                    "find": "Download Portable version[\\s\\S]+([A-F0-9]{40})"
                }
            },
            "32bit": {
                "url": "http://multicommander.com/files/updates/MultiCommander_win32_Portable_($version.$matchBuild).zip",
                "hash": {
                    "url": "http://multicommander.com/downloads",
                    "find": "Download Portable version[\\s\\S]+([A-F0-9]{40})[\\s\\S]+(?:[A-F0-9]{40})"
                }
            }
        }
    }
}
